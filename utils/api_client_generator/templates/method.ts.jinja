public async {{ method_name }}({{ param_list }}): {{ return_type }} {
    const response = await this.http.request<{{ response_type }}>({
      path: {{ path_expr }},
      method: '{{ method }}',
{% if has_query %}
      query,
{% endif %}
{% if use_json_body %}
      json: body,
{% elif use_body_payload %}
      body,
{% endif %}
    });
    return response;
  }

