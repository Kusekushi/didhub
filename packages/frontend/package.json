{
  "name": "@didhub/frontend",
  "version": "0.1.1",
  "description": "React frontend for DIDHub - a web application for managing alters and systems in DID communities",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vite build --outDir ../../static --emptyOutDir",
    "preview": "vite preview --port 5173",
    "test": "vitest",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "lint:ci": "eslint src/ --ext .js,.jsx,.ts,.tsx --max-warnings=0"
  },
  "dependencies": {
    "@didhub/api-client": "workspace:*",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@toolpad/core": "^0.16.0",
    "d3": "^7.9.0",
    "moment": "^2.30.1",
    "react": "^19.1.1",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.3",
    "rehype-sanitize": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.55.1",
    "@swc/core": "^1.13.20",
