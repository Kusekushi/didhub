{
  "name": "@didhub/frontend",
  "version": "0.3.0",
  "description": "React frontend for DIDHub - a web application for managing alters and systems in DID communities",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vite build --outDir ../../static --emptyOutDir",
    "preview": "vite preview --port 5173",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "lint:ci": "eslint src/ --ext .js,.jsx,.ts,.tsx --max-warnings=0"
  },
  "dependencies": {
    "@didhub/api-client": "workspace:*",
    "@didhub/family-tree": "workspace:*",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/x-data-grid": "^8.11.0",
    "@mui/x-date-pickers": "^8.11.0",
    "dayjs": "^1.11.18",
    "fflate": "^0.8.2",
    "lodash-es": "^4.17.21",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.3",
    "rehype-sanitize": "^6.0.0",
    "uuid": "^13.0.0"
